<AnchorPane id="AnchorPane" prefHeight="167.0" prefWidth="319.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller_view.LoginController">
    <stylesheets>
        <URL value="@/css/login.css" />
    </stylesheets>
   <children>
      <TextField fx:id="txtLogin" layoutX="13.0" layoutY="45.0" prefHeight="30.0" prefWidth="294.0" promptText="Login" text="Marcos Vinicius" />
